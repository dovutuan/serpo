{
    "version": "1.0.0",
    "name": "dovutuan/serpo",
    "description": "Laravel Repositories is used to abstract the data layer, making our application more flexible to maintain.",
    "keywords": [
        "laravel",
        "repository",
        "eloquent",
        "model",
        "cache"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dovutuan\\Serpo\\": "src/"
        }
    },
    "authors": [
        {
            "name": "dovutuan",
            "email": "tuando.0598@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "homepage": "https://github.com/dovutuan/serpo",
    "extra": {
        "laravel": {
            "providers": [
                "Dovutuan\\Serpo\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs": "phpcs --standard=PSR12 src/",
        "cbf": "phpcbf --standard=PSR12 src/"
    },
    "support": {
        "email": "tuando.0598@gmail.com",
        "issues": "https://github.com/dovutuan/serpo/issues",
        "wiki": "https://github.com/dovutuan/serpo/wiki",
        "source": "https://github.com/dovutuan/serpo",
        "docs": "https://github.com/dovutuan/serpo"
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/container": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.9"
    }
}
